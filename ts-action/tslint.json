{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "rules": {
    "adjacent-overload-signatures": { "severity": "error" },
    "align": { "severity": "off" },
    "array-type": {
      "options": ["array"],
      "severity": "error"
    },
    "arrow-parens": { "severity": "off" },
    "ban": { "severity": "off" },
    "ban-types": { "severity": "off" },
    "class-name": { "severity": "error" },
    "comment-format": { "severity": "off" },
    "completed-docs": { "severity": "off" },
    "curly": { "severity": "error" },
    "deprecation": { "severity": "warning" },
    "eofline": { "severity": "off" },
    "file-header": { "severity": "off" },
    "forin": { "severity": "error" },
    "import-blacklist": { "severity": "off" },
    "indent": {
      "options": ["spaces"],
      "severity": "error"
    },
    "interface-name": { "severity": "off" },
    "jsdoc-format": { "severity": "off" },
    "label-position": { "severity": "error" },
    "linebreak-style": { "severity": "off" },
    "match-default-export-name": { "severity": "off" },
    "max-classes-per-file": { "severity": "off" },
    "max-file-line-count": {
      "options": [0],
      "severity": "off"
    },
    "max-line-length": { "severity": "off" },
    "member-access": { "severity": "off" },
    "member-ordering": {
      "options": [
        "public-before-private",
        "static-before-instance",
        "variables-before-functions"
      ],
      "severity": "error"
    },
    "new-parens": { "severity": "error" },
    "newline-before-return": { "severity": "off" },
    "no-angle-bracket-type-assertion": { "severity": "off" },
    "no-any": { "severity": "error" },
    "no-arg": { "severity": "error" },
    "no-bitwise": { "severity": "error" },
    "no-conditional-assignment": { "severity": "error" },
    "no-consecutive-blank-lines": { "severity": "error" },
    "no-console": {
      "options": [
        "error",
        "log",
        "warn"
      ],
      "severity": "warning"
    },
    "no-consecutive-blank-lines": { "severity": "error" },
    "no-construct": { "severity": "error" },
    "no-debugger": { "severity": "error" },
    "no-default-export": { "severity": "off" },
    "no-duplicate-super": { "severity": "error" },
    "no-duplicate-variable": { "severity": "error" },
    "no-empty": { "severity": "off" },
    "no-eval": { "severity": "error" },
    "no-for-in-array": { "severity": "off" },
    "no-import-side-effect": { "severity": "off" },
    "no-inferrable-types": { "severity": "off" },
    "no-inferred-empty-object-type": { "severity": "off" },
    "no-internal-module": { "severity": "off" },
    "no-invalid-template-strings": { "severity": "error" },
    "no-invalid-this": { "severity": "error" },
    "no-magic-numbers": { "severity": "off" },
    "no-mergeable-namespace": { "severity": "error" },
    "no-namespace": {
      "options": ["allow-declarations"],
      "severity": "error"
    },
    "no-non-null-assertion": { "severity": "off" },
    "no-null-keyword": { "severity": "off" },
    "no-parameter-properties": { "severity": "off" },
    "no-reference": { "severity": "error" },
    "no-reference-import": { "severity": "error" },
    "no-require-imports": { "severity": "off" },
    "no-shadowed-variable": { "severity": "off" },
    "no-sparse-arrays": { "severity": "error" },
    "no-string-literal": { "severity": "off" },
    "no-switch-case-fall-through": { "severity": "error" },
    "no-trailing-whitespace": { "severity": "error" },
    "no-unnecessary-callback-wrapper": { "severity": "warning" },
    "no-unsafe-finally": { "severity": "error" },
    "no-unused-expression": { "severity": "error" },
    "no-use-before-declare": { "severity": "error" },
    "no-var-keyword": { "severity": "error" },
    "no-var-requires": { "severity": "off" },
    "no-void-expression": {
      "options": ["ignore-arrow-function-shorthand"],
      "severity": "error"
    },
    "object-literal-key-quotes": {
      "options": ["as-needed"],
      "severity": "off"
    },
    "object-literal-shorthand": { "severity": "off" },
    "object-literal-sort-keys": { "severity": "error" },
    "one-line": {
      "options": [
        "check-catch",
        "check-else",
        "check-open-brace",
        "check-whitespace"
      ],
      "severity": "error"
    },
    "one-variable-per-declaration": {
      "options": ["ignore-for-loop"],
      "severity": "error"
    },
    "only-arrow-functions": { "severity": "off" },
    "ordered-imports": {
      "options": [{
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }],
      "severity": "off"
    },
    "prefer-const": { "severity": "off" },
    "prefer-for-of": { "severity": "off" },
    "prefer-template": { "severity": "warning" },
    "promise-function-async": { "severity": "off" },
    "quotemark": {
      "options": ["double", "avoid-escape"],
      "severity": "error"
    },
    "radix": { "severity": "error" },
    "return-undefined": { "severity": "error" },
    "semicolon": { "severity": "error" },
    "strict-boolean-expressions": { "severity": "off" },
    "switch-default": { "severity": "error" },
    "trailing-comma": {
      "options": [{
        "multiline": "never",
        "singleline": "never"
      }],
      "severity": "error"
    },
    "triple-equals": { "severity": "error" },
    "typedef": {
      "options": [
        "call-signature",
        "parameter",
        "property-declaration"
      ],
      "severity": "error"
    },
    "typedef-whitespace": {
      "options": [{
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }, {
        "call-signature": "space",
        "index-signature": "space",
        "parameter": "space",
        "property-declaration": "space",
        "variable-declaration": "space"
      }],
      "severity": "error"
    },
    "use-isnan": { "severity": "error" },
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case",
        "allow-trailing-underscore"
      ],
      "severity": "error"
    },
    "whitespace": {
      "options": [
        "check-branch",
        "check-decl",
        "check-module",
        "check-operator",
        "check-seperator",
        "check-type"
      ],
      "severity": "error"
    }
  }
}
